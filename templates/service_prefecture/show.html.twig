{% extends 'base.html.twig' %}

{% block title %}Service Prefecture{% endblock %}

{% block body %}
    <h1>Service Prefecture</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ service_prefecture.id }}</td>
            </tr>
            <tr>
                <th>Service_nom</th>
                <td>{{ service_prefecture.serviceNom }}</td>
