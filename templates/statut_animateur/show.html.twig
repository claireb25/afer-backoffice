    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ statut_animateur.id }}</td>
            </tr>
            <tr>
                <th>Status_nom</th>
                <td>{{ statut_animateur.statusNom }}</td>
